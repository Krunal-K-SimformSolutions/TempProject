{
  "name": "sampleThunk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod-install": "npx pod-install",
    "android": "ENVFILE=.env npx react-native run-android",
    "ios": "ENVFILE=.env npx react-native run-ios",
    "android:release": "yarn lint && ENVFILE=.env.production npx react-native run-android --variant=release",
    "ios:release": "yarn lint && ENVFILE=.env.production npx react-native run-ios --configuration Release",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint './app/**/*.{js,jsx,ts,tsx}'",
    "eslint-fix": "eslint --fix './app/**/*.{js,jsx,ts,tsx}'",
    "pretty": "prettier --write './app/**/*.{js,jsx,ts,tsx}'",
    "types": "tsc --noEmit",
    "spelling": "cspell lint './app/**/*.{js,jsx,ts,tsx}'",
    "local-check": "yarn run lint && yarn run eslint-fix && yarn run pretty && yarn run types && yarn run spelling"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@react-navigation/core": "^6.3.0",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/routers": "^6.1.2",
    "@react-navigation/stack": "^6.2.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/react-native": "^4.3.0",
    "apisauce": "^2.1.6",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "formik": "^2.2.9",
    "i18next": "^21.9.1",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-i18next": "^11.18.5",
    "react-native": "^0.69.4",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-localize": "^2.2.3",
    "react-native-mmkv-storage": "^0.8.0",
    "react-native-permissions": "^3.6.1",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-spinner-button": "^1.0.9",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-decorators": "^7.19.0",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/jest": "^29.0.1",
    "@types/lodash": "^4.14.184",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "babel-jest": "^29.0.3",
    "cspell": "^6.12.0",
    "eslint": "^8.23.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "metro-react-native-babel-preset": "^0.72.2",
    "prettier": "^2.7.1",
    "react-native-flipper": "^0.164.0",
    "react-test-renderer": "^18.0.0",
    "rn-mmkv-storage-flipper": "^1.0.0",
    "redux-flipper": "^2.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.3"
  },
  "resolutions": {
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
